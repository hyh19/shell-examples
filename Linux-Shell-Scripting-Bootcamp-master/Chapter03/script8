#!/bin/sh
#
# 5/3/2017
#
echo "script8 - Linux Scripting Book"

if [ $# -eq 0 ] ; then
 echo "Please enter at least 1 parameter."
 exit 255
fi

for i in $*                  # the "$*" returns every parameter given to the script
do
 echo -n "$i "
done

echo ""                      # carriage return
echo "End of script8"

exit 0

